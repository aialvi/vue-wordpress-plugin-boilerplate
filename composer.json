{
  "name": "aialvi/aialvi-page-ranks",
  "description": "A WordPress plugin built with Vue 3, Vite, and Webpack.",
  "type": "wordpress-plugin",
  "require": {
    "php": ">=7.4",
    "composer/installers": "^2.0"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^3.1",
    "automattic/vipwpcs": "^3.0",
    "phpcompatibility/phpcompatibility-wp": "*"
  },
  "autoload": {
    "psr-4": {
      "AIALVI_Vue_Plugin\\": "src/php/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "lint:phpcs": "vendor/bin/phpcs --standard=WordPress --extensions=php --ignore=*/vendor/*,*/node_modules/*,*/release/* .",
    "fix:phpcs": "vendor/bin/phpcbf --standard=WordPress --extensions=php --ignore=*/vendor/*,*/node_modules/*,*/release/* ."
  }
}
